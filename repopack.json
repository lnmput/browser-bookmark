{
    "output": {
      "filePath": "bookmark-knowledge.txt",
      "style": "txt",
      "headerText": "a bookmark browser extension.",
      "removeComments": false,
      "removeEmptyLines": false,
      "showLineNumbers": false,
      "topFilesLength": 5
    },
    "include": ["**/*"],
    "ignore": {
      "useGitignore": true,
      "useDefaultPatterns": true,
      "customPatterns": [
        "additional-folder",
        "**/*.log",
        "build",
        "node_modules",
        "@",
        ".plasmo",
        ".github"
      ]
    }
  }